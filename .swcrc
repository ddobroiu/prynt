{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": true
    },
    "target": "es2020",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false,
    "transform": {
      "react": {
        "pragma": "React.createElement",
        "pragmaFrag": "React.Fragment",
        "throwIfNamespace": true,
        "development": false,
        "useBuiltins": false,
        "refresh": true
      }
    },
    "experimental": {
      "plugins": []
    }
  },
  "module": {
    "type": "es6",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "minify": true,
  "sourceMaps": true,
  "env": {
    "targets": {
      "chrome": "91",
      "firefox": "90", 
      "edge": "91",
      "safari": "15"
    },
    "mode": "entry",
    "debug": false,
    "dynamicImport": true,
    "loose": false,
    "skip": [
      "transform-typeof-symbol"
    ]
  }
}