{
  "crons": [
    {
      "path": "/api/cron/daily",
      "schedule": "0 9 * * *"
    }
  ],
  "functions": {
    "app/api/cron/daily/route.ts": {
      "maxDuration": 300
    }
  },
  "rewrites": [
    {
      "source": "/admin/(.*)",
      "destination": "/admin/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    }
  ]
}